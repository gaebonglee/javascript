<!--1번-->
<script>
  function big(a, b) {
    if (a > b) return a;
    else return b;
  }

  const b = big("625", "555");
  document.write("큰수= " + b);
</script>

<!--2번-->
<script>
  function pr(str, n) {
    for (let i = 0; i < n; i++) {
      document.write(str);
    }
  }
</script>

<!--3번-->
<script>
  const n = parseInt(prompt("정수를 입력하세요"));

  evenOrOdd(n);

  function evenOrOdd(number) {
    const even = "짝수";
    const odd = "홀수";
  }

  const result = number % 2 == 1 ? odd : even;

  document.write("<h1>입력한 값 : " + number + "</h1>");
  document.write("<h1>" + result + "</h1>");
</script>

