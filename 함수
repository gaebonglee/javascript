<!--6번-->
<script>
  const printStar = (n) => {
    if (isNaN(n) || n < 1) {
      document.write("입력 오류 입니다.<br>")
      return;
    }
    for (let i = 1; i <= n; i++){
      for (let j = 0; j < i; j++) {
        document.write("*");
      }
    }document.write("<br>")
  }
  const
</script>
